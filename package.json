{
	"name": "quiz_app",
	"private": true,
	"version": "0.0.0",
	"description": "quiz app repository",
	"author": "chan9yu <wry5533@gmail.com>",
	"type": "module",
	"homepage": "https://chan9yu.github.io/quiz_app/",
	"repository": {
		"type": "git",
		"url": "https://github.com/chan9yu/quiz_app"
	},
	"lint-staged": {
		"**/*.{ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		]
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build && touch ./dist/.nojekyll",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"commit": "npx git-cz",
		"prepare": "husky install",
		"husky:permission": "chmod +x .husky/*",
		"deploy": "gh-pages -d dist -t true",
		"storybook": "storybook dev -p 6006",
		"build:storybook": "storybook build"
	},
	"dependencies": {
		"@fontsource/noto-sans": "^5.0.17",
		"@reduxjs/toolkit": "^1.9.7",
		"axios": "^1.6.0",
		"chart.js": "^4.4.0",
		"immer": "^10.0.3",
		"react": "^18.2.0",
		"react-chartjs-2": "^5.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^8.1.3",
		"react-router-dom": "^6.18.0",
		"redux": "^4.2.1",
		"redux-saga": "^1.2.3",
		"styled-components": "^6.1.0",
		"styled-reset": "^4.5.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.2.0",
		"@commitlint/config-conventional": "^18.1.0",
		"@storybook/addon-essentials": "^7.5.2",
		"@storybook/addon-interactions": "^7.5.2",
		"@storybook/addon-links": "^7.5.2",
		"@storybook/blocks": "^7.5.2",
		"@storybook/builder-vite": "^7.5.2",
		"@storybook/react": "^7.5.2",
		"@storybook/react-vite": "^7.5.2",
		"@storybook/testing-library": "^0.2.2",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.9.1",
		"@typescript-eslint/parser": "^6.9.1",
		"@vitejs/plugin-react": "^4.1.0",
		"babel-plugin-styled-components": "^2.1.4",
		"commitizen": "^4.3.0",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-jest-dom": "^5.1.0",
		"eslint-plugin-prettier": "^5.0.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.4",
		"eslint-plugin-storybook": "^0.6.15",
		"gh-pages": "^6.0.0",
		"husky": "^8.0.3",
		"jsdom": "^22.1.0",
		"lint-staged": "^15.0.2",
		"prettier": "^3.0.3",
		"storybook": "^7.5.2",
		"typescript": "^5.0.2",
		"vite": "^4.4.5"
	}
}
